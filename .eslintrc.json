{
  "extends": ["next/core-web-vitals", "airbnb", "plugin:prettier/recommended"],
  "parser": "react-scripts/node_modules/babel-eslint",
  "parserOptions": { "ecmaVersion": "latest", "sourceType": "module" },
  "rules": {
    "max-len": ["error", 140],
    "quotes": [2, "single", { "avoidEscape": true }]
  }
}
